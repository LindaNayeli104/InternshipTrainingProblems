//Problem
// http://codeforces.com/contest/268/problem/A

import java.util.Scanner;

public class Games {
    	
	public static void useHostUnif(int totalGames, int[][] games) {
		
	}
	
	
	public static void main(String args[]){
        Scanner res = new Scanner(System.in);
 
        int totalGames = res.nextInt();
        int[][] games = new int[totalGames * 2][2];
        int[] uniforms = new int[2];
        int game = 0;
 
        for (int gameNum = 0; gameNum < totalGames; gameNum++){
            uniforms[0] = res.nextInt();
            uniforms[1] = res.nextInt();
            games[gameNum] = uniforms;
        }
        useHostUnif(totalGames, games);
        res.close();
    }
    
    	
}

